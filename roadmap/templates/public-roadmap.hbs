<!DOCTYPE html>
<html lang="en" >
<head>
    <meta charset="UTF-8">
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css'>
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css'>
    <link rel='stylesheet' href='https://fonts.googleapis.com/css?family=Poppins:400,100,300,700'><link rel="stylesheet" href="./style.css">

</head>
<body>
<!-- partial:index.partial.html -->
<body data-spy="scroll" data-target="#navbar-example">


<div class="wrapper" >

    <nav class="nav__wrapper" id="navbar-example">
        <ul class="nav">
            {{#each schedule}}
                <li role="presentation" {{#if @first}}class="active"{{/if}}>
                    <a href="#section{{inc @index}}">
                        <span class="nav__counter">{{name}}</span>
                        <h3  class="nav__title">{{#each items}}{{#if @first}}{{else}}, {{/if}}{{summary}}{{/each}}</h3>
                        {{#each items}}
                            <p class="nav__body">{{description}}</p>
                        {{/each}}
                    </a>
                </li>
            {{/each}}

        </ul>
    </nav>

{{#each schedule}}
    <section class="section section{{inc @index}}" id="section{{inc @index}}">
        {{name}}
    </section>
{{/each}}


</div>
</body>
<!-- partial -->
<script src='https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
<script src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js'></script><script  src="./script.js"></script>

</body>
</html>
